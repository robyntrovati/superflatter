{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": [
      "error",
      "always"
    ],
    "arrow-body-style": 0,
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "consistent-return": [
      0,
      {
        "treatUndefinedAsUnspecified": true
      }
    ],
    "import/imports-first": 0,
    "import/newline-after-import": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "max-len": 0,
    "newline-per-chained-call": 0,
    "no-console": 1,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-use-before-define": 0,
    "prefer-template": 2,
    "class-methods-use-this": 0,
    "require-yield": 0,
    "semi": [2, 'always']
  }
}
